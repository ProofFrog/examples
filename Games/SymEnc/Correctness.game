import 'Primitives/SymEnc.primitive';

Game Real(SymEnc E) {
	bool Test(E.Key k, E.Message m) {
		E.Ciphertext c = E.Enc(k, m);
		E.Message mPrime = E.Dec(k, c);
		return mPrime == m;
	}
}

Game Fake(SymEnc E) {
	bool Test(E.Key k, E.Message m) {
		return true;
	}
}

export (Real, Fake) as Correctness;
