Game Real(int lambda) {
	BitString<lambda> Eavesdrop(BitString<lambda> m) {
		BitString<lambda> k <- BitString<lambda>;
		return k + m;
	}
}

Game Random(int lambda) {
	BitString<lambda> Eavesdrop(BitString<lambda> m) {
		BitString<lambda> c <- BitString<lambda>;
		return c;
	}
}

export (Real, Random) as OTPUniform;
